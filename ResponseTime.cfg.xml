<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
 "-//Hibernate/Hibernate Mapping DTD//EN"
 "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 

<hibernate-mapping>
	<class name="ResponseTime" table="site_response_tool">
		<meta attribute="class-description">
			This contains the end user response time for each page on a website
		</meta>
		
		<id name="id" type="int" column="id">
			<generator class="native"/>
		</id>
		
		<property name="domainName" column="domain_name" type="string"/>
		<property name="pageTitle" column="page_title" type="string"/>
		<property name="pageID" column="page_id" type="string"/>
		<property name="browserName" column="browser_name" type="string"/>
		<property name="browserVersion" column="browser_version" type="string"/>
		<property name="responseTime" column="response_time" type="string"/>
		<property name="sessionID" column="session_id" type="string"/>
		<property name="dateAdded" column="added_on" type="string"/>
		
	</class>
</hibernate-mapping>